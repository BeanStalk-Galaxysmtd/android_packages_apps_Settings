<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
xmlns:android="http://schemas.android.com/apk/res/android"
        android:title="@string/about_beanstalk_settings_title"
xmlns:rb="http://schemas.android.com/apk/res/com.android.settings">

<PreferenceCategory
android:key="links"
android:title="@string/beanstalk">

<Preference
android:key="beanstalk_source"
android:title="@string/beanstalk_source_title"
android:summary="@string/beanstalk_source_summary" />

<Preference
android:key="beanstalk_facebook"
android:title="@string/beanstalk_facebook_title"
android:summary="@string/beanstalk_facebook_summary" />
</PreferenceCategory>

<PreferenceCategory
android:key="devs"
android:title="@string/devs">

<com.android.settings.widgets.DeveloperPreference
            android:title="scotthartbti"
            rb:nameDev="scotthartbti"
            rb:twitterHandle="scotthartbti"
            rb:donateLink="@string/donate_scotthartbti" />

</PreferenceCategory>

<PreferenceCategory
android:key="contributors"
android:title="@string/contributors_title">

<com.android.settings.widgets.DeveloperPreference
            android:title="AOKP"
            rb:nameDev="AOKP"
            rb:twitterHandle="aokp_rom"
            rb:donateLink="@string/donate_aokp" />

<com.android.settings.widgets.DeveloperPreference
            android:title="CyanogenMod"
            rb:nameDev="CyanogenMod"
            rb:twitterHandle="cyanogenmod"
            rb:donateLink="@string/donate_cyanogenmod" />

</PreferenceCategory>

</PreferenceScreen>
